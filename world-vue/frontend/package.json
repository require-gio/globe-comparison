    "geojson": "^0.5.0",
    "pinia": "^2.2.4",
    "three": "^0.168.0",
    "three-globe": "^2.31.1",
    "three-mesh-bvh": "^0.9.1",
    "vue": "^3.5.8"
  },
  "devDependencies": {
    "@types/node": "^20.16.5",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.15.0",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.28.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.47",
